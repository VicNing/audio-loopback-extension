  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "agora-rte-extension": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@microsoft/api-extractor": "^7.19.4",
    "babel-loader": "^8.2.3",
    "typescript": "4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "karma": "6.3.16",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "2.0.1",
    "karma-typescript": "5.5.3",
    "mocha": "9.2.0",
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.0"
  },
  "browserslist": [
